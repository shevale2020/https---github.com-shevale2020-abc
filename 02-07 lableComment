//draw line 
for (var item in Data) { 

var nodelabels = svg .append("text") .attr(
{ 
"x": xScale(format.parse(Data[item]["TaskDate"])),
 "y": yScale(Data[item]["TaskNumber"]),
 "class": "nodelubel",
 "stroke": "black" 
}) .text(function (d) {
	return Data[item]["TaskId"] 
}); 

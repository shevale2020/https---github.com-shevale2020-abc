 
 
  return context.DependentTasks.Where(rec => rec.TaskId == TaskId && rec.IsDeleted == false)
                .Select(rec => new ECloseBL.Entities.DependentTask
                { Location = rec.Task1.Team.Location.LocationName, Team = rec.Task1.Team.TeamName, DependentTaskId = rec.DependentTaskId, TeamId = rec.Task1.TeamId, Id = rec.Id }).ToList();
  
  
  
  

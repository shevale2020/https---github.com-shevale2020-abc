
//

         List<SummaryInfo>  finalSummaryList=   
(from result in from SummaryList
 select new SummaryInfo   {		
                                LocationId= result.LocationId,
                                 PastClosedWithinTAT = result.PastClosedWithinTAT,
                                 PastClosedTATMissed = result.PastClosedTATMissed,
                                PastOpenOnHold = result.PastOpenOnHold,
                                PastTotal=result.PastClosedWithinTAT+result.PastClosedTATMissed+ result.PastOpenOnHold+result.PastOpenWIP+PastOpenUnActioned,                                }).ToList<SummaryInfo>();
  .
  .
  .
  .
  .
  .
  .
  
  
  
  
})
